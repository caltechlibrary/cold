<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>edu.caltech.library.cold</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/cold</string>
        <string>/usr/local/etc/cold/settings.json</string>
    </array>
    <key>GroupName</key>
    <string>staff</string>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/usr/local/var/log/cold/access.log</string>
    <key>StandardErrorPath</key>
    <string>/usr/local/var/log/cold/errors.log</string>
    <key>Debug</key>
    <true/>
</dict>
</plist>
